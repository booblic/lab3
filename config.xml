<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Configurations>
    <Configuration>
        <Category>
            <CategoryName>lab3.logger.main</CategoryName>
        </Category>
        <AppenderLevel>
            <Appender xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ConsolAppender">
                <Filter xsi:type="MessageTextFilter" Invert="false">
                    <Key>message</Key>
                </Filter>
                <Layouts>
                    <Layout>%p %d{H:m:s,Y.M.D} %c %m %t %s</Layout>
                    <Separator> | </Separator>
                </Layouts>
            </Appender>
            <Appender xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="FileAppender" FileName="log.txt">
                <Filter xsi:type="MessageTextFilter" Invert="false">
                    <Key>System</Key>
                </Filter>
                <Layouts>
                    <Layout>%p %d{H:m:s,Y.M.D} %c %m %t %s</Layout>
                    <Separator> | </Separator>
                </Layouts>
            </Appender>
            <Level>WARN</Level>
        </AppenderLevel>
        <AppenderLevel>
            <Appender xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ConsolAppender">
                <Filter xsi:type="LevelFilter">
                    <Key>WARN</Key>
                </Filter>
                <Filter xsi:type="MessageTextFilter" Invert="false">
                    <Key>log</Key>
                </Filter>
                <Layouts>
                    <Layout>%p %d{H:m:s,Y.M.D} %c %m %t %s</Layout>
                    <Separator> | </Separator>
                </Layouts>
            </Appender>
            <Level>INFO</Level>
        </AppenderLevel>
    </Configuration>
